<ehcache>
    <diskStore path="java.io.tmpdir/omp-web"/>
    <defaultCache
        maxElementsInMemory="1000"
        eternal="false"
        timeToLiveSeconds="600"
        overflowToDisk="true"
        diskPersistent="false"
        />
    <!--=================================================================
        Hibernate Object Caches
        =================================================================-->
    <cache name="com.netease.ad.omp.entity.sys.User"
        maxElementsInMemory="100"
        timeToLiveSeconds="0"
        overflowToDisk="true"/>

</ehcache>