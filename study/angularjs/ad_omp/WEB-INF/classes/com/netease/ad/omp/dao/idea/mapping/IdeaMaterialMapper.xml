<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.netease.ad.omp.dao.idea.mapper.IdeaMaterialMapper" >
    <!--批量插入素材-->
      <insert id="batchInsertMaterial" useGeneratedKeys="true"  parameterType="java.util.List">
        INSERT INTO cr_material (material_address, idea_id, data_status,type)
        VALUES
        <foreach collection="list" item="item" index="index" separator="," >
          (
                #{item.materialAddress,jdbcType=VARCHAR},
                #{item.ideaId,jdbcType=BIGINT},
                #{item.dataStatus,jdbcType=INTEGER},
                #{item.type,jdbcType=INTEGER}
          )
        </foreach>
      </insert>
</mapper>